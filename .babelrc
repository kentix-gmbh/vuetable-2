{
  "presets": [
    ["env", { "modules": false }],
    "stage-2"
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "transform-runtime"
  ],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": [ 
        "istanbul",
        ["module-resolver", {
          "root": ["./src"],
          "alias": {
            "@": "./src"
          }
        }]
      ]
    }
  }
}
